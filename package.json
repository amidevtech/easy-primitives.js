{
  "name": "@amidevtech/easy-primitives.js",
  "version": "0.0.2",
  "description": "Provide missed functionality for remove boilerplate code when using primitives",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "primitives",
    "easy",
    "easy primitives",
    "primitives boilerplate",
    "boilerplate",
    "angular",
    "number",
    "boolean",
    "array",
    "string"
  ],
  "files": [
    "/dist"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "author": "amidevtech <info@amidev.tech>",
  "repository": "https://github.com/amidevtech/easy-primitives.js",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.6.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testPathIgnorePatterns": [
      "./src/index.ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
